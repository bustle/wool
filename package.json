{
  "name": "wooljs",
  "version": "1.5.0",
  "description": "A utility for building AWS Lambda functions",
  "main": "dist/index.js",
  "scripts": {
    "compile": "babel -d dist/ lib/",
    "lint": "standard",
    "prepublish": "npm run compile",
    "pub": "np"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bustlelabs/wool.git"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "amazon",
    "shep",
    "framework"
  ],
  "author": "Zach Foster <zachary@bustle.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bustlelabs/wool/issues"
  },
  "homepage": "https://github.com/bustlelabs/wool",
  "dependencies": {
    "airbrake": "~1.2.0",
    "shep-logger": "^4.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.2.1",
    "babel-register": "^6.11.6",
    "np": "^2.10.1",
    "standard": "^9.0.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4.3
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
